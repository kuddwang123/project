
log4j.rootLogger=INFO,file
log4j.logger.error=WARN

log4j.appender.file=org.apache.log4j.RollingFileAppender
log4j.appender.file.File=/tmp/logging/ros1.log

log4j.appender.file.MaxFileSize=10000KB
log4j.appender.file.MaxBackupIndex=2
log4j.appender.file.Append=true
log4j.appender.file.layout=org.apache.log4j.PatternLayout
log4j.appender.file.layout.ConversionPattern=[%d] [%p] (%F:%L) [%m]%n

log4j.appender.error=org.apache.log4j.RollingFileAppender
log4j.appender.error.File=/tmp/logging/my_log_err.log

log4j.appender.error.MaxFileSize=10000KB
log4j.appender.error.MaxBackupIndex=2
log4j.appender.error.Append=true
log4j.appender.error.layout=org.apache.log4j.PatternLayout
log4j.appender.error.layout.ConversionPattern=[%d] [%p] (%F:%L) [%m]%n

